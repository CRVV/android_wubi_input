<merge
    xmlns:app="http://schemas.android.com/apk/res/com.github.crvv.wubinput"
>
    <Key
        app:keySpec="~" />
    <Key
        app:keySpec="!"
        app:additionalMoreKeys="!text/morekeys_exclamation" />
    <Key
        app:keySpec="\@" />
    <Key
        app:keySpec="\#" />
    <Key
        app:keySpec="$"
        app:additionalMoreKeys="!text/morekeys_currency_dollar" />
    <Key
        app:keySpec="%"
        app:additionalMoreKeys="!text/morekeys_symbols_percent" />
    <Key
        app:keySpec="^" />
    <Key
        app:keySpec="&amp;" />
    <Key
        app:keySpec="*"
        app:additionalMoreKeys="!text/morekeys_star" />
    <Key
        app:keySpec="(" />
    <Key
        app:keySpec=")" />
    <Key
        app:keySpec="_" />
    <!-- U+00D7: "×" MULTIPLICATION SIGN
         U+00F7: "÷" DIVISION SIGN
         U+221A: "√" SQUARE ROOT -->
    <Key
        app:keySpec="+"
        app:additionalMoreKeys="!text/morekeys_plus"
        app:moreKeys="&#x00D7;,&#x00F7;,&#x221A;" />
</merge>
